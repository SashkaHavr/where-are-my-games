{
  "name": "@where-are-my-games/expo",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "android": "expo run:android",
    "build": "expo export --output-dir ./build --platform all",
    "clean": "git clean -xdf .turbo node_modules",
    "dev": "expo start",
    "eas-build-post-install": "pnpm run -w build:example",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "ios": "expo run:ios",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@where-are-my-games/prettier-config",
  "dependencies": {
    "@expo/metro-config": "^0.19.12",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@where-are-my-games/auth": "workspace:*",
    "@where-are-my-games/eslint-config": "workspace:*",
    "@where-are-my-games/prettier-config": "workspace:*",
    "@where-are-my-games/trpc": "workspace:*",
    "@where-are-my-games/ui-web": "workspace:*",
    "expo": "~52.0.26",
    "expo-blur": "~14.0.2",
    "expo-constants": "~17.0.4",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.17",
    "expo-secure-store": "^14.0.1",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.3.3"
  }
}

    "ora": "^8.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "strip-json-comments": "^5.0.1",
    "vinxi": "^0.5.3"
  },
  "devDependencies": {
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@where-are-my-games/eslint-config": "workspace:*",
    "@where-are-my-games/prettier-config": "workspace:*",
